<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.apache.ibatis.examples.DemoBeanMapper">

    <select id="selectById" parameterType="Map" resultType="org.apache.ibatis.examples.DemoBean">
        select *
        from demo
        WHERE id = #{id}
    </select>

</mapper>